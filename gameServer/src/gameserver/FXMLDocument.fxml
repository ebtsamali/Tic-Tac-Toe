<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="493.0" prefWidth="548.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gameserver.FXMLDocumentController">
    <children>
      <ToggleButton id="serverBtn" fx:id="serverBtn" layoutX="231.0" layoutY="454.0" mnemonicParsing="false" onAction="#handleButtonAction" text="turn on server" />
      <TableView fx:id="onlineUsersTable" layoutX="14.0" layoutY="14.0" prefHeight="431.0" prefWidth="524.0">
        <columns>
          <TableColumn fx:id="userName" prefWidth="102.0" text="user name" />
          <TableColumn fx:id="password" minWidth="0.0" prefWidth="104.0" text="password" />
            <TableColumn fx:id="email" minWidth="0.0" prefWidth="155.0" text="email" />
            <TableColumn fx:id="score" minWidth="0.0" prefWidth="79.0" text="score" />
            <TableColumn fx:id="state" minWidth="0.0" prefWidth="83.0" text="state" />
        </columns>
      </TableView>
      <Button fx:id="logButton" layoutX="433.0" layoutY="454.0" mnemonicParsing="false" onAction="#displayLogHistory" text="Display logs" />
    </children>
</AnchorPane>
