<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TicTacTo.DashboardController">
   <children>
      <Button layoutX="59.0" layoutY="347.0" mnemonicParsing="false" onAction="#loadSinglePlayerWindow" text="Single Player" />
      <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="10.0" prefHeight="28.0" prefWidth="730.0">
         <children>
            <ImageView fitHeight="44.0" fitWidth="43.0" onMouseClicked="#minimizeWindow">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <image>
                  <Image url="@../images/min.png" />
               </image>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ImageView>
            <ImageView fitHeight="40.0" fitWidth="40.0" onMouseClicked="#closeWindow">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <image>
                  <Image url="@../images/close.png" />
               </image>
            </ImageView>
         </children>
         <opaqueInsets>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </opaqueInsets>
      </HBox>
      <VBox fx:id="onlineUserPane" layoutX="546.0" layoutY="64.0" prefHeight="315.0" prefWidth="180.0" />
      <Button layoutX="75.0" layoutY="90.0" mnemonicParsing="false" onAction="#loadMultiPlayerWindow" text="Multiplayer" />
   </children>
</AnchorPane>
