<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="800.0" stylesheets="@../css/dbfxml.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TicTacTo.DashboardController">
   <children>
      <Button layoutX="56.0" layoutY="399.0" mnemonicParsing="false" onAction="#loadSinglePlayerWindow" prefHeight="40.0" prefWidth="119.0" style="-fx-background-color: transparent;" text="Single Player">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <VBox fx:id="onlineUserPane" layoutX="546.0" layoutY="64.0" prefHeight="315.0" prefWidth="180.0" />
      <Button layoutX="294.0" layoutY="347.0" mnemonicParsing="false" onAction="#loadMultiPlayerWindow" text="Multiplayer" />
      <Label alignment="CENTER" layoutX="577.0" layoutY="101.0" prefHeight="30.0" prefWidth="174.0" text="ONLINE PLAYERS">
         <font>
            <Font name="Comic Sans MS Bold" size="18.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="315.0" layoutY="101.0" prefHeight="30.0" prefWidth="174.0" text="TOP PLAYERS">
         <font>
            <Font name="Comic Sans MS Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="scoreLabel" alignment="CENTER" contentDisplay="CENTER" focusTraversable="false" layoutX="79.0" layoutY="282.0" prefHeight="43.0" prefWidth="92.0" text="250">
         <font>
            <Font name="Arial Rounded MT Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="Uname" contentDisplay="CENTER" focusTraversable="false" layoutX="52.0" layoutY="164.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="127.0" style="-fx-background-color: transparent;" text="USER NAME">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <ImageView fitHeight="40.0" fitWidth="40.0" layoutX="751.0" layoutY="2.0" onMouseClicked="#closeWindow">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <image>
            <Image url="@../images/close.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="44.0" fitWidth="43.0" layoutX="707.0" onMouseClicked="#minimizeWindow">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <image>
            <Image url="@../images/min.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
